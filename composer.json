{
	"name": "laravel/lumen",
	"description": "The Laravel Lumen Framework.",
	"keywords": [
		"framework",
		"laravel",
		"lumen"
	],
	"license": "MIT",
	"type": "project",
	"require": {
		"php": ">=7.1.3",
		"laravel/lumen-framework": "5.6.*",
		"vlucas/phpdotenv": "~2.2",
		"mailgun/mailgun-php": "^2.5",
		"php-http/curl-client": "^1.7",
		"guzzlehttp/psr7": "^1.4",
		"nategood/httpful": "*",
		"mercadopago/sdk": "0.5.2",
		"crabbly/fpdf-laravel": "1.0.2",
		"pusher/pusher-php-server": "^3.4.1",
		"authy/php": "^3.0",
		"phpoffice/phpspreadsheet": "^1.6",
		"picqer/php-barcode-generator": "^0.3.0",
		"league/flysystem": "^1.0",
		"firebase/php-jwt": "^5.0",
		"twilio/sdk": "^6.1",
		"nordsoftware/lumen-cors": "^3.5"
	},
	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"phpunit/phpunit": "~7.0",
		"mockery/mockery": "~1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/",
			"database/"
		]
	},
	"scripts": {
		"post-root-package-install": [
			"php -r \"copy('.env.example', '.env');\""
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"kylekatarnls/update-helper": true
		}
	}
}
